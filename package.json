{
  "name": "thank-you",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "yarn eslint --fix 'src/**/*.ts' 'src/**/*.tsx'",
    "prettier-format": "prettier --write 'src/**/*.ts' 'src/**/*.tsx'",
    "prebuild": "npx expo prebuild --clean",
    "build:ios": "eas build --profile production --platform=ios --non-interactive --auto-submit --no-wait",
    "submit:ios": "eas submit --profile production --platform=ios",
    "generate-icons-set": "cd scripts/generate-app-icon && yarn install && yarn generate-icons",
    "eas-build-pre-install": "yarn generate-icons-set --value=$(node scripts/utils/get-animations-count.js)"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.3",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-native-seoul/masonry-list": "^1.4.2",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@shopify/react-native-skia": "1.5.0",
    "@shopify/restyle": "^2.4.4",
    "color": "^4.2.3",
    "date-fns": "^3.6.0",
    "expo": "~52.0.44",
    "expo-asset": "~11.0.5",
    "expo-blur": "~14.0.3",
    "expo-constants": "~17.0.8",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-image": "~2.0.7",
    "expo-linear-gradient": "~14.0.2",
    "expo-sensors": "~14.0.2",
    "expo-status-bar": "~2.0.1",
    "fft.js": "^4.0.4",
    "jotai": "^2.9.3",
    "lodash": "^4.17.21",
    "lottie-react-native": "7.1.0",
    "moti": "^0.27.2",
    "pressto": "^0.2.2-beta.2",
    "qrcode": "^1.5.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-fast-confetti": "^0.8.2",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-qrcode-skia": "^0.2.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-redash": "^18.1.2",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-skia-gesture": "^0.3.6",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.13",
    "react-query": "^3.39.3",
    "simplex-noise": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/color": "^3.0.6",
    "@types/lodash": "^4.17.7",
    "@types/qrcode": "^1.5.5",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-native-wcandillon": "^3.10.0",
    "eslint-plugin-ft-flow": "^3.0.11",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.1.1",
    "typescript": "~5.3.3"
  },
  "private": true
}
