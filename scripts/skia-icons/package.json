{
  "name": "skia-icons-generator",
  "version": "2.0.0",
  "description": "Generate beautiful icons using Headless Skia with multiple themes",
  "main": "build/generator.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "generate": "tsc && node build/generator.js",
    "generate:ios-home-grid": "yarn build && node build/generator.js --theme=ios-home-grid",
    "clean": "rm -rf build",
    "dev": "tsc --watch"
  },
  "devDependencies": {
    "@shopify/react-native-skia": "v2.0.0-next.4",
    "@types/node": "^22.5.4",
    "@types/react": "^19.0.10",
    "react": "^19.0.0",
    "typescript": "^5.5.4"
  }
}
